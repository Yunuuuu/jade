        [
            "Positron.exe",
            "Positron"
        ]
    ],
    "checkver": {
        "github": "https://github.com/posit-dev/positron",
        "regex": "\\/releases\\/tag\\/([\\d.\\-]+)"
    },
    "autoupdate": {
        "url": "https://cdn.posit.co/positron/dailies/win/x86_64/Positron-$version-UserSetup.exe"
